<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.liyihuanx.myjetpack">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".JetPackApplication"
        android:allowBackup="true"
        android:icon="@drawable/icon_jetpack"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".practice.coordinatorlayout.CollapsingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".practice.coordinatorlayout.CoordinatorActivity"/>
        <activity android:name=".practice.vlayout.VLayoutActivity" />
        <activity android:name=".practice.SecondActivity">
        </activity>
        <activity android:name=".main.MainActivity">

        </activity>
    </application>

</manifest>